    <div class="container p-5 bg-light" id="search-container">
        <div id="search">


            <h2>Search Reddit <i class="fab fa-reddit-alien"></i></h2>
            <div class="mb-3">
                <input type="text" class="form-control" id="search-form" placeholder="Search">
            </div>
            <h5>Sort By :</h5>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="sortby" id="relevance" checked="" value="Relevance">
                <label class="form-check-label" for="relevance">
                    Relevance
                </label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="sortby" id="latest" value="new">
                <label class="form-check-label" for="flexRadioDefault2">
                    Latest
                </label>
            </div>
            <h5>Limit</h5>
            <div class="mb-3">
                <input type="text" class="form-control" id="limit" placeholder="Enter the number of articles to be shown e.g 25,100">
            </div>
            <div class="d-grid gap-2">
                <button class="btn btn-dark" type="button" id="search-btn">Search</button>

            </div>
        </div>
